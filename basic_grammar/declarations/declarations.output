   REC
   WHERE


Grammar

    0 $accept: g_arbre_exp $end

    1 g_arbre_exp: var_local_in

    2 var_local_in: declaration_var_local IN g_arbre_exp
    3             | declaration_var_local IN expression

    4 declaration_var_local: LET affectation

    5 affectation: IDEN '=' affectation
    6            | expression
    7            | g_arbre_exp

    8 expression: arbre
    9           | ARITH_EXPR

   10 arbre: balise chaine CLOSE

   11 chaine: chaine intermediaire
   12       | /* empty */

   13 intermediaire: EMPTY_NODE
   14              | texte
   15              | arbre
   16              | IDEN

   17 texte: TEXT

   18 balise: TAG
   19       | TAG suite_cle_valeur

   20 suite_cle_valeur: suite_cle_valeur KEY VALUE
   21                 | KEY VALUE


