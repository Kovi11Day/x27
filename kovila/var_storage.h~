
static const int init_size = 3;

struct variable_t{
  char* var_name;
  double var_value;
};

typedef struct variable_t* variable;

struct storage_t{
  variable* tab;
  int num_elements;
  int size;
};

typedef struct storage_t* storage;


storage create_storage (void);
void destroy_storage (storage s);

void update_storage(storage s, char* name, double value);
int get_value (storage s, char* name);
